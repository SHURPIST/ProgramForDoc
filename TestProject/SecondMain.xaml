<Window x:Class="TestProject.SecondMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TestProject"
        mc:Ignorable="d"
        Title="Рыба" Name="SecondWindow" WindowState="Maximized" BorderThickness="0" MinHeight="620" MinWidth="1250" Background="#F2F2F2">
        <Window.Resources>
            <SolidColorBrush x:Key="BorderToButtonMenu" Color="#343434"></SolidColorBrush>
            <SolidColorBrush x:Key="ForegroundToButtonMenu" Color="#cccccc"></SolidColorBrush>
        </Window.Resources>
    
        <Grid ShowGridLines="False" Name="gridmain">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MinWidth="365"></ColumnDefinition>
                    <ColumnDefinition MinWidth="315" MaxWidth="400"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            
            <ScrollViewer Grid.Row="1" Grid.Column="2">
                <DockPanel Margin="5,0,0,0"> 
                    <AccessText x:Name="FileBlock" FontSize="16" FontWeight="Bold" Background="White"></AccessText>
                </DockPanel>    
            </ScrollViewer>
                <DockPanel Name="ToolPanel" Grid.Row="0" Grid.RowSpan="1" Grid.Column="0" Grid.ColumnSpan="3" Background="#404040">
                    <Grid ShowGridLines="False">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="56"></ColumnDefinition>
                            <ColumnDefinition MinWidth="250" Name="ColumnAddPacWeidth"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition Width="115"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        
                        <Button Click="ClearSearch" Grid.Column="0" BorderBrush="Transparent" BorderThickness="0" Foreground="#cccccc" FontWeight="Bold" Background="#151515" MinWidth="21" MaxWidth="29" Height="19" Content="X"></Button>
                        <Button Click="SeetingsClick" Name="Seetings" Grid.Column="5" BorderBrush="Transparent" BorderThickness="0" Foreground="#cccccc" FontWeight="Bold" Background="#151515" Width="110" HorizontalAlignment="Left" Content="Настройки"></Button>
                        <Button Name="AddPacientka" Click="AddPacientkuPZ" Grid.Column="2" MinWidth="145" Height="21" Background="#151515" Foreground="#cccccc" BorderBrush="Transparent" BorderThickness="0" FontWeight="Bold" HorizontalAlignment="Left" Margin="15 0 0 0">Добавить пациентку</Button>
                        <TextBox Grid.Column="1"  Name="SearchPacientok" Margin="0 2 0 2" Background="#151515" Foreground="#cccccc" BorderBrush="Transparent" BorderThickness="0" FontWeight="Bold" TextChanged = "TextBoxChanged" MinWidth="200" Height="18"></TextBox>
                    </Grid>
                </DockPanel>
                <StackPanel Grid.Column="0" Grid.Row="1" ZIndex="200" Name="DockPanelLeftMenu" Visibility="Visible">
                    <ScrollViewer HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden">
                        <StackPanel x:Name="gridLayout"></StackPanel>
                    </ScrollViewer>
                </StackPanel>
                <StackPanel  Name="StackMenu" HorizontalAlignment="Right" Grid.Column="2" Grid.Row="1" Background="Transparent" Visibility="Hidden"  Margin="0 0 5 0">
                    <ScrollViewer VerticalScrollBarVisibility="Hidden">
                        <DockPanel Name="StackMenuPanel">
                            <StackPanel>
                                <Button Name="SelectedSaveFileDirect" BorderBrush="Transparent" BorderThickness="0" Foreground="#cccccc" FontWeight="Bold" Background="#343434" Height="22" Width="110" Content="Сохранить в.." Click="SelectedCreateFileDirectClick"></Button>
                                <Button Name="SelectedFindFileDirect" BorderBrush="Transparent" BorderThickness="0" Foreground="#cccccc" FontWeight="Bold" Background="#343434" Height="22" Width="110" Content="Искать в.." Click="SelectedFindFileDirectClick"></Button>
                            </StackPanel>
                        </DockPanel>
                    </ScrollViewer>
                </StackPanel>
                <Button Name="PrintThisFile" Grid.Row="1" Grid.Column="2" BorderBrush="Transparent" BorderThickness="0" Foreground="#cccccc" FontWeight="Bold" Background="#343434" Height="25" Width="95" HorizontalAlignment="Right" Content="Печать" VerticalAlignment="Bottom" Margin="0 0 35 20" Click="PrintTheFile">
                    <Button.Effect>
                        <DropShadowEffect Color="#606060" ShadowDepth="0" BlurRadius="15"></DropShadowEffect>
                    </Button.Effect>
                </Button>
                <Button Name="ActivationVoice" Grid.Row="1" Grid.Column="2" BorderBrush="Transparent" BorderThickness="0" Foreground="#cccccc" FontWeight="Bold" Background="#343434" Height="25" Width="95" HorizontalAlignment="Right" Content="Гол.ввод" VerticalAlignment="Bottom" Margin="0 0 145 20" Click="ActivationGolVvod">
                    <Button.Effect>
                        <DropShadowEffect Color="#606060" ShadowDepth="0" BlurRadius="15"></DropShadowEffect>
                    </Button.Effect>
                </Button>
                <ScrollViewer VerticalScrollBarVisibility="Hidden" Margin="0 5 0 0" Grid.Column="0" Grid.Row="1" Visibility="Visible">
                    <DockPanel Margin="10 0 10 0">
                        <StackPanel>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenStart"  x:Name="shab_start" >Шаблон начала</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenLive" x:Name="anal_live" >Анамнез жизни</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenAnam" x:Name="gin_anam" >Гинекологический анамнез</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenIsxodBer" x:Name="isxod_ber" >Исход предыдущих беременностей</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenTechBer" x:Name="tech_ber" >Течение данной беременности</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenObject" x:Name="dan_object_obs" >Данные объективного обследования</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenIss" x:Name="vlaznoe_iss" >Влагалищное исследование</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenShcala" x:Name="shkala_riska" >Шкала перинатального риска</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="OpenPlan" x:Name="plan_rodov" >План ведения родов:</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold" Click="naznach" >Назначения</Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold"></Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold"></Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold"></Button>
                            <Button Height="45" MinWidth="320" Margin="0 10 0 0" FontSize="16" FontWeight="Bold"></Button>
                        </StackPanel>
                    </DockPanel>
                </ScrollViewer>
                <DockPanel Grid.Column="1" Grid.Row="1" Background="Transparent" Margin="5 10 5 0" x:Name="shablon_nach_a4" Visibility="Visible">
                    <Grid ShowGridLines="True">
                        <ScrollViewer Name="ContenA4" >
                            <DockPanel>
                                <Grid ShowGridLines="True">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    
                                    <DockPanel Grid.Row="0">
                                        <TreeView Name="DataAndTime" Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                            <TreeViewItem Header="Дата и время" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                <DockPanel>
                                                    <Grid ShowGridLines="False">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition></ColumnDefinition>
                                                            <ColumnDefinition Width="180"></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition Height="5"></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        
                                                        <TextBlock Foreground="#343434" FontWeight="Bold" FontSize="14" Grid.Column="0" Grid.Row="0">Дата:</TextBlock>
                                                        <TextBlock Foreground="#343434" FontWeight="Bold" FontSize="14" Grid.Column="0" Grid.Row="2">Время:</TextBlock>
                                                        <TextBox Grid.Row="0" Grid.Column="1" Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="DateText" x:Name="Date"></TextBox>
                                                        <TextBox Grid.Row="2" Grid.Column="1" Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="TimeText" x:Name="Time"></TextBox>
                                                    </Grid> 
                                                </DockPanel>
                                            </TreeViewItem>
                                        </TreeView>
                                    </DockPanel>
                                    <DockPanel Name="FIO" Grid.Row="1" Margin="0 10 0 0">
                                        <TreeView Name="FIO_View" Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                            <TreeViewItem Header="Личные данные" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                <DockPanel>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition></ColumnDefinition>
                                                            <ColumnDefinition Width="180"></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition Height="5"></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition Height="5"></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        
                                                        <TextBlock Foreground="#343434" FontWeight="Bold" FontSize="14" Grid.Column="0" Grid.Row="0">История родов №:</TextBlock>
                                                        <TextBlock Foreground="#343434" FontWeight="Bold" FontSize="14" Grid.Column="0" Grid.Row="2">Ф.И.О.</TextBlock>
                                                        <TextBlock Foreground="#343434" FontWeight="Bold" FontSize="14" Grid.Column="0" Grid.Row="4">Возраст</TextBlock>
                                                        <TextBox Grid.Row="0" Grid.Column="1" Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="HisCreateText" x:Name="StoryCreate"></TextBox>
                                                        <TextBox Grid.Row="2" Grid.Column="1" Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="familyBox" x:Name="0Ф.И.О:"></TextBox>
                                                        <TextBox Grid.Row="4" Grid.Column="1" Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="textBoxAdd" x:Name="1Возраст:"></TextBox>
                                                    </Grid>
                                                </DockPanel>
                                            </TreeViewItem>
                                        </TreeView>
                                    </DockPanel>
                                    <DockPanel Name="Diraction" Grid.Row="2" Margin="0 10 0 0">
                                        <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                            <TreeViewItem Header="Поступила по направлению:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                <RadioButton Foreground="#343434" FontSize="14" FontWeight="Bold" Content="СМП" GroupName="Diraction" Checked="SMPVector"></RadioButton>
                                                <RadioButton Foreground="#343434" FontSize="14" FontWeight="Bold" Content="Врача ЖК" GroupName="Diraction" Checked="DockVector"></RadioButton>
                                                <RadioButton Foreground="#343434" FontSize="14" FontWeight="Bold" Content="Самостоятельно" GroupName="Diraction" Checked="SelfVector"></RadioButton>
                                            </TreeViewItem>
                                        </TreeView>
                                    </DockPanel>
                                    <DockPanel Name="DstoPostup" Grid.Row="3" Margin="0 10 0 0">
                                        <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                            <TreeViewItem Header="DS при поступлении:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="DSText" x:Name="DSgo"></TextBox>
                                            </TreeViewItem>
                                        </TreeView>
                                    </DockPanel>
                                    <DockPanel Name="LiveZalabi" Grid.Row="4" Margin="0 10 0 0">
                                        <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                            <TreeViewItem Header="Жалобы при поступлении:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="10 0 5 0" Width="200" TextChanged="ErText" x:Name="AngryGo"></TextBox>
                                            </TreeViewItem>
                                        </TreeView>
                                    </DockPanel>
                                </Grid>
                            </DockPanel>
                        </ScrollViewer>
                    </Grid>
                </DockPanel>
                <DockPanel Name="Anamnezlivetable" Grid.Column="1" Grid.Row="1" Margin="5 10 5 0" Visibility="Hidden">
                    <ScrollViewer HorizontalScrollBarVisibility="Hidden" >
                        <DockPanel Name="onziz">
                            <Grid ShowGridLines="False">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                
                                <DockPanel Grid.Row="0" Name="Commonzabol" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Общие заболевания:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="ColdDiases">Простудные</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="GiperDiases">Гипертоническая болезнь</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="SugarDiases">Сахарный диабет</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="AllDiasesText" x:Name="AllDiasesAdd"></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="SocialDiraction" Grid.Row="1" Margin="0 10 0 0">
                                    <DockPanel>
                                        <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                            <TreeViewItem Header="Социальное положение:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="GoodState">Благополучное</RadioButton>
                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="BadState">Неблагополучное</RadioButton>
                                            </TreeViewItem>
                                        </TreeView>
                                    </DockPanel>
                                </DockPanel>
                                <DockPanel Name="Smoking" Grid.Row="2" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Курение:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoSmoke">Нет</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesSmoke">1 пачка в день</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="SmokeText" x:Name="SmokingTextBox"></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="ProfHazards" Grid.Row="3" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Профессиональные вредности:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesProfHar">Есть</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoProfHar">Нет</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="ProfHarText" ></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="Gemotrasnfuz" Grid.Row="4" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Гемотрансфузии:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesGemotransf">Были</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoGemotransf">Не были</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="GemotrnsfText"></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="Drugs" Grid.Row="5" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Наркотики:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesDrugs">Принимала</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoDrugs">Не принимала</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="DrugsText"></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="VichBol" Grid.Row="6" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Контакт с инфекционными больными ВИЧ,гепатит, Tbs, Lues:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesVhich">Имела</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoVhich">Не имела</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="VichText"></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="AllergAnaliz" Grid.Row="7" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Аллергологический анамнез:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesAnam">Отягощён</RadioButton>
                                            <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoAnam">Не отягощён</RadioButton>
                                            <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="AnamText"></TextBox>
                                        </TreeViewItem>
                                    </TreeView>
                                </DockPanel>
                                <DockPanel Name="Hushband" Grid.Row="8" Margin="0 10 0 0">
                                    <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                        <TreeViewItem Header="Муж:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                            <Grid ShowGridLines="False">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                
                                                <TextBlock Foreground="#343434" FontWeight="Bold" FontSize="14" Margin="0 10 0 0">Возраст:</TextBlock>
                                                <TextBox Grid.Row="0" Grid.Column="0" Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="80 10 0 0" Width="200" TextChanged="husOld"></TextBox>
                                                
                                                <DockPanel Name="NextInfromationToHushband" Margin="0 10 0 0" Grid.Row="1" Grid.ColumnSpan="1">
                                                    <StackPanel Margin="0 10 0 0">
                                                        <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                                            <TreeViewItem Header="Хронические заболевания:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="YesHronDiases">Есть</RadioButton>
                                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="NoHronDiases">Нет</RadioButton>
                                                                <TextBox Height="23" Background="#343434" Foreground="#cccccc" FontWeight="Bold" Margin="0 10 0 0" Width="200" TextChanged="HronDiasesText"></TextBox>
                                                            </TreeViewItem>
                                                        </TreeView>
                                                        <TreeView Background="Transparent" BorderBrush="Transparent" BorderThickness="0" FontSize="15" FontWeight="Bold" Foreground="#343434">
                                                            <TreeViewItem Header="Группа крови:" FontSize="15" FontWeight="Bold" Foreground="#343434" Background="Transparent">
                                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="GroupOne">OI</RadioButton>
                                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="GroupTwo">AII</RadioButton>
                                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="GroupThree">BIII</RadioButton>
                                                                <RadioButton Margin="0 10 0 0" FontSize="14" Foreground="#343434" FontWeight="Bold" Checked="GroupForthy">ABIV</RadioButton>
                                                            </TreeViewItem>
                                                        </TreeView>
